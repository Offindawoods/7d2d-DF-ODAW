<config>
	<set xpath="/spawning/biome[@name='pine_forest']">
		<spawn maxcount="4" respawndelay="1" time="Day" entitygroup="ZombiesDay" tags="commercial,industrial,downtown" />
		<spawn maxcount="4" respawndelay="1" time="Night" entitygroup="ZombiesNight" tags="commercial,industrial,downtown" />
		
		<spawn maxcount="2" respawndelay="1.25" time="Day" entitygroup="ZombiesDay" tags="residential,rural,gateway,countryresidential,countrytown" />
		<spawn maxcount="2" respawndelay="1.25" time="Night" entitygroup="ZombiesNight" tags="residential,rural,gateway,countryresidential,countrytown" />
			
		<spawn maxcount="1" respawndelay="2" time="Day" entitygroup="ZombiesDay" notags="commercial,industrial,downtown,residential,rural,gateway,countryresidential,countrytown" />
		<spawn maxcount="2" respawndelay="1" time="Night" entitygroup="ZombiesNight" notags="commercial,industrial,downtown,residential,rural,gateway,countryresidential,countrytown" />
			
		<spawn maxcount="1" respawndelay="5" time="Day" entitygroup="EnemyAnimalsForest" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" />
		<spawn maxcount="3" respawndelay="10" time="Day" entitygroup="FriendlyAnimalsForest" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" /> 
			
		<spawn maxcount="4" respawndelay="5" time="Night" entitygroup="EnemyAnimalsForest" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" />
		<spawn maxcount="2" respawndelay="10" time="Night" entitygroup="FriendlyAnimalsForest" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" /> 
			
		<spawn maxcount="1" respawndelay="20" time="Any" entitygroup="MeleeSurvivorT1" />
	</set>
	
	<set xpath="/spawning/biome[@name='burnt_forest']">
		<spawn maxcount="4" respawndelay="7" time="Any" entitygroup="ZombiesBurntForest" />
		<spawn maxcount="4" respawndelay="4" time="Night" entitygroup="ZombiesBurntForestNight" />
			
		<spawn maxcount="1" respawndelay="5" time="Any" entitygroup="EnemyAnimalsBurntForest" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" />
		<spawn maxcount="3" respawndelay="10" time="Any" entitygroup="FriendlyAnimalsBurntForest" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" /> 
			
		<!-- <spawn maxcount="1" respawndelay="15" time="Any" entitygroup="MeleeBanditT2" /> -->
	</set>
	
	<set xpath="/spawning/biome[@name='desert']">
		<spawn maxcount="4" respawndelay="0.5" time="Day" entitygroup="ZombiesDay" tags="commercial,industrial,downtown" />
		<spawn maxcount="4" respawndelay="0.5" time="Night" entitygroup="ZombiesNight" tags="commercial,industrial,downtown" />
			
		<spawn maxcount="2" respawndelay="0.75" time="Day" entitygroup="ZombiesDay" tags="residential,rural,gateway,countryresidential,countrytown" />
		<spawn maxcount="2" respawndelay="0.75" time="Night" entitygroup="ZombiesNight" tags="residential,rural,gateway,countryresidential,countrytown" />
			
		<spawn maxcount="1" respawndelay="2" time="Day" entitygroup="ZombiesDay" notags="commercial,industrial,downtown,residential,rural,gateway,countryresidential,countrytown" />
		<spawn maxcount="1" respawndelay="1" time="Night" entitygroup="ZombiesNight" notags="commercial,industrial,downtown,residential,rural,gateway,countryresidential,countrytown" />
			
		<spawn maxcount="1" respawndelay="5" time="Day" entitygroup="EnemyAnimalsDesert" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" />
		<spawn maxcount="3" respawndelay="10" time="Day" entitygroup="FriendlyAnimalsDesert" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" /> 
			
		<spawn maxcount="4" respawndelay="5" time="Night" entitygroup="EnemyAnimalsDesert" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" />
		<spawn maxcount="2" respawndelay="10" time="Night" entitygroup="FriendlyAnimalsDesert" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" /> 
			
		<spawn maxcount="1" respawndelay="20" time="Any" entitygroup="MeleeSurvivorT2" /> <!--
		<spawn maxcount="1" respawndelay="40" time="Any" entitygroup="MeleeBanditT1" /> -->
	</set>
		
	<set xpath="/spawning/biome[@name='snow']">
		<spawn maxcount="4" respawndelay="0.25" time="Day" entitygroup="SnowZombies" tags="commercial,industrial,downtown" />
		<spawn maxcount="4" respawndelay="0.25" time="Night" entitygroup="SnowZombiesNight" tags="commercial,industrial,downtown" />
			
		<spawn maxcount="2" respawndelay="0.5" time="Day" entitygroup="SnowZombies" tags="residential,rural,gateway,countryresidential,countrytown" />
		<spawn maxcount="2" respawndelay="0.5" time="Night" entitygroup="SnowZombiesNight" tags="residential,rural,gateway,countryresidential,countrytown" />
			
		<spawn maxcount="1" respawndelay="2" time="Day" entitygroup="SnowZombies" notags="commercial,industrial,downtown,residential,rural,gateway,countryresidential,countrytown" />
		<spawn maxcount="1" respawndelay="1" time="Night" entitygroup="SnowZombiesNight" notags="commercial,industrial,downtown,residential,rural,gateway,countryresidential,countrytown" />
			
		<spawn maxcount="1" respawndelay="5" time="Day" entitygroup="EnemyAnimalsSnow" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" />
		<spawn maxcount="3" respawndelay="10" time="Day" entitygroup="FriendlyAnimalsSnow" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" /> 
			
		<spawn maxcount="4" respawndelay="5" time="Night" entitygroup="EnemyAnimalsSnow" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" />
		<spawn maxcount="2" respawndelay="10" time="Night" entitygroup="FriendlyAnimalsSnow" spawnDeadChance="0" notags="commercial,industrial,downtown,countryresidential,countrytown,residential" /> 
			
		<spawn maxcount="1" respawndelay="20" time="Any" entitygroup="MeleeSurvivorT3" /> <!--
		<spawn maxcount="1" respawndelay="40" time="Any" entitygroup="MeleeBanditT2" /> -->
	</set>
	
	<set xpath="/spawning/biome[@name='wasteland']">
		<spawn maxcount="3" respawndelay="0.25" time="Day" entitygroup="ZombiesWasteland2" tags="commercial,industrial,downtown,residential,rural,gateway,countryresidential,countrytown" />
		<spawn maxcount="3" respawndelay="0.25" time="Night" entitygroup="ZombieDemonsBiome" tags="commercial,industrial,downtown,residential,rural,gateway,countryresidential,countrytown" />
			
		<spawn maxcount="2" respawndelay="0.375" time="Day" entitygroup="ZombiesWasteland2" notags="commercial,industrial,downtown,residential,rural,gateway,countryresidential,countrytown" />
		<spawn maxcount="2" respawndelay="0.25" time="Night" entitygroup="ZombiesWastelandNight" notags="commercial,industrial,downtown,residential,rural,gateway,countryresidential,countrytown" />

		<spawn maxcount="1" respawndelay="0.5" time="Day" entitygroup="EnemyAnimalsWasteland" spawnDeadChance="0" />
		<spawn maxcount="1" respawndelay="0.25" time="Night" entitygroup="EnemyAnimalsWastelandHub" spawnDeadChance="0" />
	</set>
	
	<insertAfter xpath="/spawning/entityspawner[@name='Zombie_Ghost_Town_Sm_Territorial']">
		<!-- Zombie Football Stadium Spawns -->
		<entityspawner name="Zombie_Football_Stadium_Med_Territorial">

			<day value="*">
				<property name="ResetToday" value="false" />

				<property name="EntityGroupName" value="ZombieFootballStadiumGroup" />
				<property name="Time" value="Any" />

				<property name="DelayBetweenSpawns" value="0" />
				<!-- in seconds -->
				<property name="TotalAlive" value="6" />

				<property name="TotalPerWave" value="6" />
			<property name="DaysToRespawnIfPlayerLeft" value="5" /> <!-- in game days -->

				<property name="AttackPlayerAtOnce" value="false" />

				<property name="Territorial" value="false" />
				<property name="TerritorialRange" value="100" />
				</day>
		</entityspawner>
	</insertAfter>
</config>